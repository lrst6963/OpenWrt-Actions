
config aria2 'main'
	option user 'aria2'
	option bt_enable_lpd 'true'
	option enable_dht 'true'
	option follow_torrent 'true'
	option file_allocation 'none'
	option save_session_interval '30'
	list header ''
	list extra_settings ''
	option rpc_listen_port '6800'
	option rpc_auth_method 'token'
	option rpc_secret '13594759'
	option config_dir '/etc/aria2'
	option enable_log 'false'
	option disk_cache '128M'
	option enabled '1'
	option dir '/mnt/mmcblk0p3/filebrowser'

